<?xml version="1.0"?>
<package >
  <metadata>
    <id>Synthesis.Solr</id>
    <version>$version$</version>
    <authors>Ben Lipson,Kam Figy,Jeff Darchuk</authors>
    <owners>Synthesis Team</owners>
    <licenseUrl>http://opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>https://github.com/blipson89/Synthesis</projectUrl>
    <iconUrl>http://www.benlipson.net/wp-content/uploads/2018/12/synthesis-logo.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Adds Solr support to Synthesis. This package contains the core library and configuration and is appropriate for the web project.

      NOTE: Synthesis 9.1.0 and above requires Sitecore 9.1.0 or higher
    </description>
    <copyright>Copyright 2021 Ben Lipson/Synthesis Team</copyright>
    <tags>sitecore,synthesis,solr</tags>
    <dependencies>
      <dependency id="Synthesis" version="[$version$]" />
      <dependency id="Synthesis.Solr.Core" version="[$version$]" />
    </dependencies>
    <contentFiles>
      <files include="any/**/*" buildAction="Content" copyToOutput="true" />
    </contentFiles>
  </metadata>
  <files>
    <file src="..\..\Source\Synthesis.Solr\Standard Config Files\*" target="contentFiles\any\any\App_Config\Include\Synthesis" />

    <!-- Legacy Support -->
    <file src="..\..\Source\Synthesis.Solr\Standard Config Files\Synthesis.Solr.config" target="content\App_Config\Include\Synthesis" />
  </files>
</package>